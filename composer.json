{
    "name": "juit/garage-door-control",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "JUIT\\GDC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JUIT\\GDC\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "juit/raspberry-piface2-api": "dev-master",
        "myclabs/php-enum": "^1.5",
        "roave/security-advisories": "dev-master",
        "symfony/process": "^3.3"
    },
    "require-dev": {
        "juit/mailhog-client": "^1.0",
        "phpunit/phpunit": "^6.4"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/juitde/mailhog-client"
        },
        {
            "type": "vcs",
            "url": "https://github.com/janunger/raspberry-piface2-api"
        }
    ]
}
